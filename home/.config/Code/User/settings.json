  "editor.minimap.maxColumn": 80,
  "editor.minimap.scale": 2,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 100, 120, 200],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "eslint.options": {
    "extensions": [".js", ".jsx"]
  },
  "explorer.confirmDragAndDrop": false,
  "diffEditor.renderSideBySide": true,
  "files.trimTrailingWhitespace": true,
  "githubPullRequests.showInSCM": true,
  "githubPullRequests.defaultMergeMethod": "rebase",
  "gitlens.codeLens.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.views.compare.location": "scm",
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.location": "scm",
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.search.location": "scm",
  "go.formatTool": "gofmt",
  "go.lintTool": "staticcheck",
  "go.testFlags": ["-v"],
  "go.useLanguageServer": true,
  "html.format.wrapLineLength": 80,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "liveshare.allowGuestDebugControl": true,
  "liveshare.featureSet": "stable",
  "liveshare.sharedTerminalHeight": 100,
  "liveshare.sharedTerminalWidth": 160,
  "search.location": "panel",
  "terraform.indexing": {
    "enabled": false,
    "liveIndexing": false,
    "delay": 500,
    "exclude": [
      ".terraform/**/*",
      "**/.terraform/**/*"
    ]
  },
  "terraform.languageServer": {
    "enabled": true,
    "args": []
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-u>": true
  },
  "vim.useSystemClipboard": true,
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Night Owl",
  "workbench.editor.labelFormat": "medium",
  "workbench.editor.tabSizing": "shrink",
  "workbench.startupEditor": "newUntitledFile",
  "html.mirrorCursorOnMatchingTag": false,
}
