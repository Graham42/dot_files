{
  "editor.formatOnSave": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 100, 120, 200],
  "editor.tabSize": 2,
  "eslint.options": {
    "extensions": [".js", ".jsx"]
  },
  "files.trimTrailingWhitespace": true,
  "workbench.editor.labelFormat": "medium",
  "vim.useSystemClipboard": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-u>": true
  },
  "workbench.editor.tabSizing": "shrink",
  "workbench.startupEditor": "newUntitledFile",
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "Dismissible",
    "admin",
    "analytics",
    "backend",
    "blogging",
    "cancelable",
    "checkbox",
    "checklist",
    "ciphertext",
    "cleartext",
    "committer",
    "composable",
    "debounced",
    "defcon",
    "deregistration",
    "deserialize",
    "dialog",
    "endregion",
    "essentialism",
    "fallback",
    "fqdn",
    "frontmatter",
    "hmmm",
    "idempotency",
    "junit",
    "keyframes",
    "kubernetes",
    "leveraging",
    "lifecyle",
    "malware",
    "mcgregor",
    "microservice",
    "microservices",
    "minikube",
    "monorepo",
    "monorepos",
    "oneliner",
    "pageable",
    "particularly",
    "pings",
    "placeholder",
    "placeholders",
    "polyfill",
    "postgres",
    "precached",
    "precommit",
    "prerequest",
    "promisify",
    "proxied",
    "rearchitecture",
    "recurse",
    "refactor",
    "refactoring",
    "repo",
    "screenshots",
    "scrollable",
    "serverless",
    "setup",
    "shortcut",
    "sigkill",
    "sigterm",
    "snackbar",
    "subdomain",
    "subdomains",
    "subheader",
    "templating",
    "testcase",
    "testcases",
    "timeout",
    "toolbar",
    "truthy",
    "unguessable",
    "unmount",
    "unrecognized",
    "unsanitized",
    "unserialized",
    "uuid",
    "versioning",
    "vulns",
    "webapp",
    "webhook",
    "webhooks",
    "whitelisting",
    "workflow",
    "workflows"
  ],
  "html.format.wrapLineLength": 80,
  "search.location": "panel",
  "window.zoomLevel": 0,
  "explorer.confirmDragAndDrop": false,
  "prettier.proseWrap": "always",
  "[xml]": {
    "editor.formatOnSave": false
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[markdown]": {
    "editor.quickSuggestions": false
  },
  "go.formatTool": "gofmt"
}
