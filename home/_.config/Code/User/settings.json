{
  "editor.formatOnSave": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 100, 120, 200],
  "editor.tabSize": 2,
  "eslint.options": {
    "extensions": [".js", ".jsx"]
  },
  "files.trimTrailingWhitespace": true,
  "workbench.editor.labelFormat": "medium",
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-u>"],
      "after": ["2", "0", "k"]
    },
    {
      "before": ["<C-d>"],
      "after": ["2", "0", "j"]
    }
  ],
  "vim.handleKeys": {
    "<C-d>": true
  },
  "workbench.editor.tabSizing": "shrink",
  "workbench.startupEditor": "newUntitledFile",
  "cSpell.language": "en-GB",
  "cSpell.userWords": [
    "admin",
    "analytics",
    "cancelable",
    "checkbox",
    "composable",
    "debounced",
    "defcon",
    "deregistration",
    "deserialize",
    "endregion",
    "essentialism",
    "frontmatter",
    "hmmm",
    "junit",
    "keyframes",
    "lifecyle",
    "malware",
    "mcgregor",
    "microservices",
    "minikube",
    "oneliner",
    "pageable",
    "placeholder",
    "polyfill",
    "precommit",
    "prerequest",
    "promisify",
    "rearchitecture",
    "recurse",
    "refactoring",
    "repo",
    "scrollable",
    "serverless",
    "setup",
    "subheader",
    "templating",
    "truthy",
    "unguessable",
    "unmount",
    "vulns",
    "webhook",
    "webhooks",
    "whitelisting",
    "workflow",
    "workflows"
  ],
  "html.format.wrapLineLength": 80,
  "search.location": "panel",
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Default Light+",
  "explorer.confirmDragAndDrop": false,
  "prettier.proseWrap": "always"
}